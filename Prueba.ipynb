    "#Ejercicio 8\n",
    "try:\n",
    "    x=input(\"ingresa un numero\")\n",
    "    numero=float(x)\n",
    "    print(f\"El resultado de 10/{x} es: {10/numero}\")\n",
    "except ValueError:\n",
    "    print(f\"El valor '{x}' no es numerico\")\n",
    "except ZeroDivisionError:\n",
    "    print(\"Error: division por cero \")"
